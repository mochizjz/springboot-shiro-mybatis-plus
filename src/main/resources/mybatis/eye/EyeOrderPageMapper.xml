	<select id="selectOrderPageNum"  resultType="int">
 	  select count(1) from eye_order_page where date(create_time) = curdate();
    </select>



	<select id="selectPageListForNode" parameterType="EyeOrderPage" resultMap="EyeOrderPageResult">
        select node_code,max(record_time) AS record_time from eye_order_page
