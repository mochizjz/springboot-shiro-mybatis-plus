    <sql id="selectEyeOrderPageVo">
        select id,token,sys_code,sdk_type,order_id,page_id,node_code,user_id,begin_time,end_time,record_time,play_set_time,file_path,file_size,client_type,client_ip,client_city_code,client_city_name,client_browser,policy_no,policy_name,policy_idcard,status,oper_type,node_url,create_user,create_time,update_user,update_time,flag,remark from eye_order_page
    </sql>

    <select id="selectEyeOrderPageList" parameterType="EyeOrderPage" resultMap="EyeOrderPageResult">
        <include refid="selectEyeOrderPageVo"/>
        <where>  
            <if test="token != null  and token != ''"> and token = #{token}</if>
            <if test="orderId != null  and orderId != ''"> and order_id = #{orderId}</if>
            <if test="pageId != null  and pageId != ''"> and page_id = #{pageId}</if>
            <if test="nodeCode != null  and nodeCode != ''"> and node_code = #{nodeCode}</if>
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            
            <!-- <if test="beginTime != null "> and begin_time = #{beginTime}</if> -->
             <if test="params.beginTime != null and params.beginTime != ''"><!-- 开始时间检索 -->
				and date_format(begin_time,'%y%m%d') &gt;= date_format(#{params.beginTime},'%y%m%d')
			</if>
			<if test="params.endTime != null and params.endTime != ''"><!-- 结束时间检索 -->
				and date_format(begin_time,'%y%m%d') &lt;= date_format(#{params.endTime},'%y%m%d')
			</if>
            <if test="recordTime != null "> and record_time = #{recordTime}</if>
            <if test="filePath != null  and filePath != ''"> and file_path = #{filePath}</if>
            <if test="clientType != null  and clientType != ''"> and client_type = #{clientType}</if>
            <if test="clientIp != null  and clientIp != ''"> and client_iP = #{clientIp}</if>
            <if test="clientCityCode != null  and clientCityCode != ''"> and client_city_code = #{clientCityCode}</if>
            <if test="clientCityName != null  and clientCityName != ''"> and client_city_name like concat('%', #{clientCityName}, '%')</if>
            <if test="clientBrowser != null  and clientBrowser != ''"> and client_browser = #{clientBrowser}</if>
             <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="createUser != null  and createUser != ''"> and create_user = #{createUser}</if>
            <if test="updateUser != null  and updateUser != ''"> and update_user = #{updateUser}</if>
            <if test="flag != null  and flag != ''"> and flag = #{flag}</if>
            <if test="operType != null  and operType != ''"> and oper_type = #{operType}</if>
            <if test="policyNo != null  and policyNo != ''"> and policy_no = #{policyNo}</if>
            <if test="policyName != null  and policyName != ''"> and policy_name like concat('%', #{policyName}, '%')</if>
            <if test="policyIdcard != null  and policyIdcard != ''"> and policy_idcard = #{policyIdcard}</if>
            <if test="nodeUrl != null  and nodeUrl != ''"> and node_url = #{nodeUrl}</if>
            <if test="sysCode != null  and sysCode != ''"> and sys_code = #{sysCode}</if>
            <if test="sdkType != null  and sdkType != ''"> and sdk_type = #{sdkType}</if>
         </where>
        order by create_time desc
    </select>
    
    <select id="selectEyeOrderPageById" parameterType="Long" resultMap="EyeOrderPageResult">
        <include refid="selectEyeOrderPageVo"/>
        where id = #{id}
