<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ins.webeye.project.eye.ordercheck.mapper.EyeOrderCheckMapper">
    
    <resultMap type="EyeOrderCheck" id="EyeOrderCheckResult">
        <result property="id"    column="id"    />
        <result property="sysCode"    column="sys_code"    />
        <result property="orderId"    column="order_id"    />
        <result property="productCode"    column="product_code"    />
        <result property="policyNo"    column="policy_no"    />
        <result property="policyName"    column="policy_name"    />
        <result property="policyIdcard"    column="policy_idcard"    />
        <result property="token"    column="token"    />
        <result property="payStatus"    column="pay_status"    />
        <result property="checkStatus"    column="check_status"    />
        <result property="checkMessage"    column="check_message"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="flag"    column="flag"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectEyeOrderCheckVo">
        select id, sys_code, order_id, product_code, policy_no, policy_name, policy_idcard, token, pay_status,check_status, check_message, create_by, create_time, update_by, update_time, flag, remark from eye_order_check
    </sql>

    <select id="selectEyeOrderCheckList" parameterType="EyeOrderCheck" resultMap="EyeOrderCheckResult">
        <include refid="selectEyeOrderCheckVo"/>
        <where>  
            <if test="sysCode != null  and sysCode != ''"> and sys_code = #{sysCode}</if>
            <if test="orderId != null  and orderId != ''"> and order_id = #{orderId}</if>
            <if test="productCode != null  and productCode != ''"> and product_code = #{productCode}</if>
            <if test="checkStatus != null  and checkStatus != ''"> and check_status = #{checkStatus}</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> 
            and create_time between #{params.beginCreateTime} and #{params.endCreateTime}
            </if>
        </where>
            order by create_time desc
    </select>
    
    <select id="selectEyeOrderCheckById" parameterType="Long" resultMap="EyeOrderCheckResult">
        <include refid="selectEyeOrderCheckVo"/>
        where id = #{id}
    </select>
    
    <select id="selectEyeOrderCheckByOrderId" parameterType="String" resultMap="EyeOrderCheckResult">
        <include refid="selectEyeOrderCheckVo"/>
        where order_id = #{orderId}
    </select>
        
        
    <insert id="insertEyeOrderCheck" parameterType="EyeOrderCheck" useGeneratedKeys="true" keyProperty="id">
        insert into eye_order_check
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sysCode != null and sysCode != ''">sys_code,</if>
            <if test="orderId != null and orderId != ''">order_id,</if>
            <if test="productCode != null">product_code,</if>
            <if test="policyNo != null">policy_no,</if>
            <if test="policyName != null">policy_name,</if>
            <if test="policyIdcard != null">policy_idcard,</if>
            <if test="token != null">token,</if>
            <if test="payStatus != null and payStatus != ''">pay_status,</if>
            <if test="checkStatus != null and checkStatus != ''">check_status,</if>
            <if test="checkMessage != null and checkMessage != ''">check_message,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="flag != null">flag,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sysCode != null and sysCode != ''">#{sysCode},</if>
            <if test="orderId != null and orderId != ''">#{orderId},</if>
            <if test="productCode != null">#{productCode},</if>
            <if test="policyNo != null">#{policyNo},</if>
            <if test="policyName != null">#{policyName},</if>
            <if test="policyIdcard != null">#{policyIdcard},</if>
            <if test="token != null">#{token},</if>
            <if test="payStatus != null and payStatus != ''">#{payStatus},</if>
            <if test="checkStatus != null and checkStatus != ''">#{checkStatus},</if>
            <if test="checkMessage != null and checkMessage != ''">#{checkMessage},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="flag != null">#{flag},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateEyeOrderCheck" parameterType="EyeOrderCheck">
        update eye_order_check
        <trim prefix="SET" suffixOverrides=",">
            <if test="sysCode != null and sysCode != ''">sys_code = #{sysCode},</if>
            <if test="orderId != null and orderId != ''">order_id = #{orderId},</if>
            <if test="productCode != null">product_code = #{productCode},</if>
            <if test="policyNo != null">policy_no = #{policyNo},</if>
            <if test="policyName != null">policy_name = #{policyName},</if>
            <if test="policyIdcard != null">policy_idcard = #{policyIdcard},</if>
            <if test="token != null">token = #{token},</if>
            <if test="payStatus != null and payStatus != ''">pay_status = #{payStatus},</if>
            <if test="checkStatus != null and checkStatus != ''">check_status = #{checkStatus},</if>
            <if test="checkMessage != null and checkMessage != ''">check_message = #{checkMessage},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="flag != null">flag = #{flag},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteEyeOrderCheckById" parameterType="Long">
        delete from eye_order_check where id = #{id}
    </delete>

    <delete id="deleteEyeOrderCheckByIds" parameterType="String">
        delete from eye_order_check where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>