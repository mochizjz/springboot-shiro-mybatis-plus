<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ins.webeye.project.eye.pageconfig.mapper.EyePageConfigMapper">
    
    <resultMap type="EyePageConfig" id="EyePageConfigResult">
        <result property="id"    column="id"    />
        <result property="sysCode"    column="sys_code"    />
        <result property="pageUrl"    column="page_url"    />
        <result property="pageName"    column="page_name"    />
        <result property="nodeCode"    column="node_code"    />
        <result property="excludeContent"    column="exclude_content"    />
        <result property="addJsFunction"    column="add_js_function"    />
        <result property="status"    column="status"    />
        <result property="createUser"    column="create_user"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateUser"    column="update_user"    />
        <result property="updateTime"    column="update_time"    />
        <result property="flag"    column="flag"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectEyePageConfigVo">
        select id, sys_code, page_url, page_name, node_code, exclude_content, add_js_function, status, create_user, create_time, update_user, update_time, flag, remark from eye_page_config
    </sql>

    <select id="selectEyePageConfigList" parameterType="EyePageConfig" resultMap="EyePageConfigResult">
        <include refid="selectEyePageConfigVo"/>
        <where>  
            <if test="sysCode != null  and sysCode != ''"> and sys_code = #{sysCode}</if>
            <if test="pageUrl != null  and pageUrl != ''"> and page_url like concat('%', #{pageUrl}, '%')</if>
            <if test="pageName != null  and pageName != ''"> and page_name like concat('%', #{pageName}, '%')</if>
            <if test="nodeCode != null  and nodeCode != ''"> and node_code = #{nodeCode}</if>
            <if test="excludeContent != null  and excludeContent != ''"> and exclude_content = #{excludeContent}</if>
            <if test="addJsFunction != null  and addJsFunction != ''"> and add_js_function = #{addJsFunction}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="createUser != null  and createUser != ''"> and create_user = #{createUser}</if>
            <if test="updateUser != null  and updateUser != ''"> and update_user = #{updateUser}</if>
            <if test="flag != null  and flag != ''"> and flag = #{flag}</if>
        </where>
        order by create_time desc
    </select>
    
    <select id="selectEyePageConfigById" parameterType="Long" resultMap="EyePageConfigResult">
        <include refid="selectEyePageConfigVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertEyePageConfig" parameterType="EyePageConfig" useGeneratedKeys="true" keyProperty="id">
        insert into eye_page_config
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sysCode != null">sys_code,</if>
            <if test="pageUrl != null">page_url,</if>
            <if test="pageName != null">page_name,</if>
            <if test="nodeCode != null">node_code,</if>
            <if test="excludeContent != null">exclude_content,</if>
            <if test="addJsFunction != null">add_js_function,</if>
            <if test="status != null">status,</if>
            <if test="createUser != null">create_user,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateUser != null">update_user,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="flag != null">flag,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sysCode != null">#{sysCode},</if>
            <if test="pageUrl != null">#{pageUrl},</if>
            <if test="pageName != null">#{pageName},</if>
            <if test="nodeCode != null">#{nodeCode},</if>
            <if test="excludeContent != null">#{excludeContent},</if>
            <if test="addJsFunction != null">#{addJsFunction},</if>
            <if test="status != null">#{status},</if>
            <if test="createUser != null">#{createUser},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateUser != null">#{updateUser},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="flag != null">#{flag},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateEyePageConfig" parameterType="EyePageConfig">
        update eye_page_config
        <trim prefix="SET" suffixOverrides=",">
            <if test="sysCode != null">sys_code = #{sysCode},</if>
            <if test="pageUrl != null">page_url = #{pageUrl},</if>
            <if test="pageName != null">page_name = #{pageName},</if>
            <if test="nodeCode != null">node_code = #{nodeCode},</if>
            <if test="excludeContent != null">exclude_content = #{excludeContent},</if>
            <if test="addJsFunction != null">add_js_function = #{addJsFunction},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createUser != null">create_user = #{createUser},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateUser != null">update_user = #{updateUser},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="flag != null">flag = #{flag},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteEyePageConfigById" parameterType="Long">
        delete from eye_page_config where id = #{id}
    </delete>

    <delete id="deleteEyePageConfigByIds" parameterType="String">
        delete from eye_page_config where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>